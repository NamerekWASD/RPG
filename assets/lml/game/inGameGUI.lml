<?xml version="1.0"?>
<!DOCTYPE dialog SYSTEM "dtd\lml.dtd">

<dialog id="dialog" movable="false">
    <table fillparent="true" defaultpad="16" onecolumn="true">
        <textbutton onchange="close">@continue</textbutton>
        <textbutton onchange="goto:menu">@gotoMenu</textbutton>
    </table>
</dialog>